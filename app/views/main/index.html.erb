<% content_for :title do %>techstori<% end %>

<% content_for :head do %>
<script type="text/javascript">
  var counter;
  var running = 0;
  
  $(document).ready(function() {
    resizePanel();
    $(window).resize(resizePanel);
    $('#left a').click(function() {
      $.post("<%= root_url %>", { keyword: $(this).text() }, null, "script");
    });
  });
  
  function resizePanel() {
    $('#right').width( $(document).width() - $('#left').width() );
  }
</script>
<% end %>
